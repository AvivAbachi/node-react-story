		"cors": "^2.8.5",
		"express": "^4.18.2",
		"express-rate-limit": "^6.6.0",
		"express-validator": "^6.14.2",
		"generate-password": "^1.7.0",
		"jsonwebtoken": "^8.5.1",
		"pg": "^8.8.0"
	},
	"devDependencies": {
		"nodemon": "^2.0.20",
		"prisma": "^4.6.0"
	}
}
